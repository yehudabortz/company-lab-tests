
<%= form_with model: @user, url: login_path do  |f| %>
    <%= f.label :email, class: "label" %>
    <%= f.text_field :email, class: "input" %> <br></br>
    <%= f.label :password, class: "label" %>
    <%= f.password_field :password, class: "input" %> <br></br>
    <div class="control">
        <%= f.submit "Log In", class: "button is-link" %>
    </div>
<% end %>
<br>
<%= render 'google_button' %>