<%= form_for [:admin, @test] do |f| %>
    <div class="field">
        <%= f.label :unique_test_id, class: "label"  %>
        <%= f.text_field :unique_test_id, :readonly => true, class: "input" %>
    </div>
    <div class="field">
        <%= f.label :mma, class: "label" %>
        <%= f.text_field :mma, class: "input" %>
    </div>
    <div class="field">
        <%= f.label :creatinine, class: "label" %>
        <%= f.text_field :creatinine, class: "input" %>
    </div>
    <div class="field">
        <%= f.label :verified, class: "label" %>
        <%= f.check_box :verified, class: "checkbox" %>
    </div>
    <div class="control">
        <%= f.submit class: "button is-link" %>
    </div>
<% end %>