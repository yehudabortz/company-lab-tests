<div class="container">
    <div class="container-box auto-grid-rows max-w-five">
        <%= form_for @test, url: tests_registration_path, html: {class:"form-stretch"}  do |f| %>
            <%= f.label "Unique Test ID", class: "label" %>
            <%= f.text_field :unique_test_id, class: "input margin-5" %>
            
            <%= f.submit "Register", class: "button is-link" %>
        <% end %>
    </div>
</div>
