<%= form_for @company do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br>
    
    <%= f.fields_for :users do |f| %> <br>
       <%= f.label :first_name %> <br>
       <%= f.text_field :first_name %> <br>
       <%= f.password_field :password %> <br>
    <% end %> <br>
    <%= f.submit %> <br>
<% end %>